{

  "builds": [{
        "src": "todo_project/wsgi.py",
        "use": "@vercel/python",
//    "config": {"maxLambdaSize": "255mb","runtime": "python3.10.5" }
  },
    {
    "src":"build.sh",
    "use":"@vercel/static-bold",
    "config":{
      "distDir": "staticfiles"

    }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "todo_project/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }

  ]


}